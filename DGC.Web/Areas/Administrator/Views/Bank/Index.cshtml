<style>
    #mydiv {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 1000;
        background-color: grey;
        opacity: .6;
    }

    .ajax-loader {
        position: absolute;
        left: 50%;
        top: 50%;
