<body ng-app="NusPayApp" class="layout-boxed-body" data-offset="40" style="background-color: white !important;min-height: 100%!important;  padding-bottom:35px !important">
    <header ng-controller="LayoutPageCtrl">
        @{
            const string header = "~/Views/Shared/_Header.cshtml";
            Html.RenderPartial(header);
        }
    </header>
    <div style="display:inline;align-self:center;align-content:center;" class="layout-boxed-body00">
        <div class="container">
            <form action="/Account/SaveRegister" class="form-horizontal">
                <h4>Create a new account.</h4>
                <hr />

                @*<div class="form-group">
                    <label class="col-md-2 control-label">Mobile</label>
                    <div class="col-md-3">
                        <input type="text" class="form-control" name="MobileNumber" id="MobileNumber" value="@Model.MobileNumber" /><label class="col-md-4">(User Name)</label>
                    </div>
                </div>*@
                <div class="form-group">
                    <label class="col-md-2 control-label">Email</label>
                    <div class="col-md-3">
                        <input type="text" class="form-control" name="Email" id="Email" value="@Model.Email" />
                    </div>
                     <label  class="col-md-6 control-label"  value="[User Name]" />
                    
                </div>
                <div class="form-group">
                    <label class="col-md-2 control-label">Full Name</label>
                    <div class="col-md-3">
                        <input type="text" class="form-control" name="FullName" id="FullName" value="@Model.FullName" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-2 control-label">Account Number</label>
                    <div class="col-md-3">
                        <input type="text" class="form-control" name="AccountNumber" id="AccountNumber" value="@Model.AccountNumber" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-2 control-label">Password</label>
                    <div class="col-md-3">
                        <input type="password" class="form-control" name="Password" id="Password" value="@Model.Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-2 control-label">Confirm Password</label>
                    <div class="col-md-3">
                        <input type="password" class="form-control" name="ConfirmPassword" id="ConfirmPassword" value="@Model.ConfirmPassword" />
                    </div>
                </div>
                @*<div class="form-group">
                    <label class="col-md-2 control-label">Customer Status</label>
                    <div class="col-md-3">
                        <input type="password" class="form-control" readonly="readonly" name="ConfirmPassword" id="ConfirmPassword" value="New" />
                    </div>
                </div>*@
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-3">
                        <input type="submit" class="btn btn-default" value="Register" />
                    </div>
                </div>
            </form>
        </div>
    </div>
    <footer class="sticky-footer" style=" background-color: #243972 !important; margin-left: -50px !important;">
        <div class="container">
            &nbsp;&nbsp;&nbsp; &copy; 2019-
            <script>document.write(new Date().getFullYear());</script>  KBZ Bank Ltd.
        </div>
    </footer>
    @Styles.Render("~/Content/assets")
    <script src="~/assets/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js"></script>
    <script src="~/assets/plugins/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/assets/js/login.js"></script>
    <script src="~/assets/plugins/bootstrap-fileupload/bootstrap-fileupload.min.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            jQuery("#menu li a").click(function (event) {
                // check if window is small enough so dropdown is created
                jQuery("#nav-collapse").removeClass("in").addClass("collapse");
            });
        });
    </script>

</body>
